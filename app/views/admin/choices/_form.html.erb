<%= form_with url: admin_choices_url, model:choice, local: true do |f| %>
    <div class="form-group">
      <%= f.label :choice_name%>
      <%= f.text_field :choice_name, class: 'form-control' %>
    </div>
    <%= f.hidden_field :question_id, value: @question.id %>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>